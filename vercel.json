{
  "version": 2,
  "builds": [
    {
      "src": "BACKEND/server.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "BACKEND/server.cjs"
    }
  ],
  "env": {
    "DB_HOST": "@db_host",
    "DB_DATABASE": "@db_database",
    "DB_USER": "@db_user",
    "DB_PASSWORD": "@db_password",
    "DB_PORT": "@db_port"
  }
}
